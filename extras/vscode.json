{    
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "Hack, 'Droid Sans Mono', monospace",
    "editor.folding": false,
    "editor.fontSize": 16,
    "editor.formatOnSave": false,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 1000,
    "editor.renderWhitespace": "selection",
    "files.hotExit": "off",
    "window.restoreWindows": "none",
    "window.zoomLevel": 1,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Oceanic Next",
    "workbench.colorCustomizations": {
        "activityBar.background": "#09141B",
        "editor.background": "#09141B",
        "editorGroupHeader.tabsBackground": "#0d1d27",
        "input.background": "#09141B",
        "sideBar.background": "#0d1d27",        
        "tab.activeBackground": "#09141B",
        "tab.border": "#20465d",
        "tab.inactiveBackground": "#0d1d27",
        "tab.unfocusedInactiveBackground": "#0d1d27"
    },
    "workbench.settings.editor": "json",
    "workbench.tree.indent": 16,
    "workbench.iconTheme": null,
    // Extension specific
    "go.formatTool": "goimports",
    "eslint.autoFixOnSave": true,
    // Filetype Specific Settings
    "[markdown]": {
        "editor.fontFamily": "Hack, \"Droid Sans Mono\", monospace",
        "editor.fontLigatures": true,
        "editor.fontSize": 16,
        "editor.lineHeight": 26,
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 72,
        "editor.lineNumbers": "off",
        "editor.quickSuggestions": false,
    },
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",

}
